{%- if not title -%}
    {%- set title = site.title -%}
{%- endif -%}
<!doctype html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width,initial-scale=1">
    <!-- <link rel="preload" href="/web/css/fonts/lato/2.0/LatoLatin-Regular.supersubset.woff2" as="font" type="font/woff2" crossorigin=""> -->
    <title>{{ title }}</title>
    <meta property="og:title" content="{{ title }}">
    <meta name="author" content="{{ site.author }}">
    <meta property="og:site_name" content="{{ site.title }}">
    <meta name="description" content="{{ description }}">
    <meta property="og:description" content="{{ description }}">
    <link rel="stylesheet" href="{{ '/css/main.min.css' | url }}" type="text/css">
    {% for stylesheet in cssStylesheets %}
        <link rel="stylesheet" href="{{ stylesheet | url }}" type="text/css">
    {% endfor %}
    {% include "components/matomo.html" %}
</head>

<body>
    {{ content | safe }}
</body>

</html>